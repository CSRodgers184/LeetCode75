Real Code

class Solution:
    def reverseVowels(self, s: str) -> str:
        vowels = set("aeiouAEIOU")
        stk = [c for c in s if c in vowels]
        s = list(s)
        for i, c in enumerate(s):
            if c in vowels:
                s[i] = stk.pop()
        return "".join(s)


